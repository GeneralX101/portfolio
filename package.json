{
  "name": "janczizikow-portfolio",
  "version": "1.0.0",
  "repository": "github:janczizikow/janczizikow-portfolio",
  "author": "Jan Czizikow <jan.czizikow@gmail.com>",
  "license": "MIT",
  "private": false,
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "eslint --fix",
      "prettier --config .prettierrc --write",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "stylelint --config=.stylelintrc --fix",
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "scripts": {
    "clean": "gatsby clean",
    "start": "yarn run dev",
    "develop": "yarn run dev",
    "dev": "gatsby develop",
    "prebuild": "cd plugins/gatsby-source-strapi && npm i && npm run build",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "format": "prettier --write 'src/**/*.{js,jsx}'",
    "lint:css": "stylelint --config=.stylelintrc 'src/**/*.{js, jsx}'",
    "lint:js": "eslint --config=.eslintrc --fix 'src/**/*.{js,jsx}'",
    "lint": "yarn lint:css && yarn lint:js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@styled-system/prop-types": "^5.1.5",
    "@styled-system/should-forward-prop": "^5.1.5",
    "body-scroll-lock": "^3.0.2",
    "dotenv": "^8.2.0",
    "emotion-theming": "^10.0.27",
    "facepaint": "^1.2.1",
    "formik": "^2.1.4",
    "gatsby": "^2.22.13",
    "gatsby-image": "^2.4.5",
    "gatsby-plugin-canonical-urls": "^2.3.2",
    "gatsby-plugin-emotion": "^4.3.2",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-google-analytics": "^2.3.2",
    "gatsby-plugin-layout": "^1.3.2",
    "gatsby-plugin-manifest": "^2.4.9",
    "gatsby-plugin-netlify": "^2.3.3",
    "gatsby-plugin-netlify-cache": "^1.2.0",
    "gatsby-plugin-nprogress": "^2.3.2",
    "gatsby-plugin-offline": "^2.2.10",
    "gatsby-plugin-preact": "^4.0.0",
    "gatsby-plugin-react-helmet": "^3.3.2",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sharp": "^2.6.9",
    "gatsby-plugin-sitemap": "^2.4.3",
    "gatsby-plugin-typography": "^2.5.2",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-source-filesystem": "^2.3.8",
    "gatsby-transformer-sharp": "^2.5.3",
    "preact": "^10.4.4",
    "preact-render-to-string": "^5.1.9",
    "prop-types": "^15.6.2",
    "react": "16.13.1",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "16.13.1",
    "react-headroom": "^3.0.0",
    "react-helmet": "^6.0.0",
    "react-icons": "^3.10.0",
    "react-modal": "^3.11.2",
    "react-pose": "^4.0.10",
    "react-typography": "^0.16.19",
    "styled-system": "^5.1.5",
    "typography": "^0.16.19",
    "typography-theme-bootstrap": "^0.16.19",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-preset-gatsby": "^0.2.18",
    "eslint": "6.1.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^3.0.9",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-emotion": "^10.0.32",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.13.1",
    "stylelint": "^11.1.1",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/jest-preprocess.js"
    },
    "testRegex": "/(.*(test|spec))\\.jsx?$",
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "snapshotSerializers": [
      "jest-emotion/serializer"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ]
  }
}
